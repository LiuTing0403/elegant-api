    "test-rd-server": "babel-node ./test/server/rd.jsx",
    "test-watch": "karma start",
    "test": "babel-node --plugins transform-runtime ./test/server/start.jsx",
    "pub": "npm run lint && webpack && webpack -p"
  },
  "files": [
    "dist/",
    "src/",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/qiu8310/elegant-api.git"
